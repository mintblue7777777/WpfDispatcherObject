<Window
    x:Class="WpfDispatcherObject.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WpfDispatcherObject"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <StackPanel>
        <Button x:Name="OKButton" Content="UIスレッドからなのでOK" Click="OKButton_Click" />
        <Button x:Name="NGButton" Content="UIスレッド以外からなのでNG" Click="NGButton_Click" />
        <Button x:Name="DispacherButton" Content="UIスレッド以外からDispacher経由で呼ぶのでOK" Click="DispacherButton_Click" />

    </StackPanel>
</Window>
